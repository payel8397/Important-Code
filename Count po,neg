#include<stdio.h>
int main()
{
    int t;
    scanf("%d",&t);
    int x;
    int zero=0,pos=0,neg=0;

    for(int i=1;i<=t;i++)
    {
        scanf("%d",&x);
        if(x==0) zero++;
        else if(x>0) pos++;
        else if(x<0) neg++;
    }
    printf("Positive: %d Negetive: %d Zero: %d\n",pos,neg,zero);
    return 0;
